}

.contextmenu li.oam_hover{
	background:#eee;
}

	.contextmenu li a{
		display:block;
	}

.contextmenu#context_bc{
	top: 103px;
	left: 80px;
}

/* Search Overlay */
.contextmenu#context_search{
