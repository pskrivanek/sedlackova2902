	left:10px;
	top:7px;
}

button.date span{
	padding:4px 2px 0 7px;
}

button#addSegment {
	float:right;
	margin-right:10px;
	min-width:145px;
	margin-top:10px;
}

	button#addSegment span.btn_add{
		background:url("../img/icons/add_16.png") left no-repeat;
		height:16px;
		width:16px;
		padding:0 0 0 10px;
		display:block;
		float:left;
	}
	
	button#addSegment span.text{
		padding:0;
		margin:0;
		float:left;
		background:none;
	}

/*Large Button (max. button width = 200PX!)*/
button.btn_large{
	background:url("../img/btn_large_right.png") right no-repeat;
	height:42px;
	position:relative;
}

	button.btn_large span{
		background:url("../img/btn_large_left.png") left no-repeat;
		padding:14px 5px 0 10px;
		height:28px;
		font-size:14px;
	}
	
	div.button-bar{
		margin-left:145px;
		margin-top:105px;
	}
	
	button#restart span{
		background:url("../img/btn_restart_left.png") left no-repeat;
		padding:14px 5px 0 45px;
	}
	
	button#stop span{
		background:url("../img/btn_stop_left.png") left no-repeat;
		padding:14px 5px 0 45px;
	}
	
	button#start span{
		background:url("../img/btn_start_left.png") left no-repeat;
		padding:14px 5px 0 45px;
	}
	
button.btn_large:hover{
	background:url("../img/btn_large_right_hover.png") right no-repeat;
}

	button.btn_large:hover span{
		background:url("../img/btn_large_left_hover.png") left no-repeat;
	}
	
	button#restart:hover span{
			background:url("../img/btn_restart_left_hover.png") left no-repeat;
		}
	
	button#start:hover span{
			background:url("../img/btn_start_left_hover.png") left no-repeat;
		}
		
		button#stop:hover span{
			background:url("../img/btn_stop_left_hover.png") left no-repeat;
		}
		
button.oam_toggle_active, button.oam_toggle_active:hover{
	background:url("../img/btn_toggle_right.png") right no-repeat;
}

	button.oam_toggle_active span, button.oam_toggle_active:hover span{
		background:url("../img/btn_toggle_left_active.png") left no-repeat;
		color:#fff;
	}

/*Toggle Button Bar (max. button width = 200PX!)*/
.oam_toggle button{
	margin:0px;
	padding: 0 10px;
	height:22px;
	float:left;
	color:#4c4c4c;
}
		
	.oam_toggle button.left{
		background:url("../img/btn_toggle_left_std.png") left no-repeat;
	}
			
	.oam_toggle button.mid{
		background:url("../img/btn_toggle_mid_std.png") top repeat-x;
	}			
	
	.oam_toggle button.right{
		background:url("../img/btn_toggle_right_std.png") right no-repeat;
	}
	
	/* Spacer to avoid border-issues in IE7 */
	.oam_toggle .spacer{
		background-color: #5b6264;
		height:22px;
		width:1px;
		float:left;
	}
	
	/* Hover and active State */
	.oam_toggle button.oam_toggle_active{
		color:white;
	}
	
	.oam_toggle button.left:hover{
		background:url("../img/btn_toggle_left_hover.png") left no-repeat;
	}
	
	.oam_toggle button.mid:hover{
		background:url("../img/btn_toggle_mid_hover.png") top repeat-x;
	}
	
	.oam_toggle button.right:hover{
		background:url("../img/btn_toggle_right_hover.png") right no-repeat;
	} 
	
	.oam_toggle button.left_active{
		background:url("../img/btn_toggle_left_active.png") left no-repeat;
	}
	
	.oam_toggle button.mid_active{
		background:url("../img/btn_toggle_mid_active.png") top repeat-x;
	}
	
	.oam_toggle button.right_active{
		background:url("../img/btn_toggle_right_active.png") right no-repeat;
	}	
/***********************
MAINCONTENT > SUBHEADER
***********************/	

div#maincontent .subheader{
	width: 100%;
	height: 28px;
	background:url("../img/subheader_bg.png") repeat-x;
	border-top:1px solid #949999;
	position: relative;
}

	div#maincontent .subheader .oam_toggle{
