/******************
SPLITTER
******************/
#splitter {
	/*height: 1000px; /*Initial Value, will be overwritten by JS*/
	/* No padding allowed */
}
/*
 * Top element of the splitter. Use pixel units for the
 * min-height and max-height; the splitter plugin parses them to
 * determine the splitter movement limits. Set the height to
 * the desired initial height of the element; the plugin changes
 * the height of this element dynamically.
 */
/* 
 * Splitter bar style; the .active class is added when the
 * mouse is over the splitter or the splitter is focused
 * via the keyboard taborder or an accessKey. 
 */
#splitter .hsplitbar {
	height: 6px;
	background: #ddd url(../img/splitter_bg.png) repeat-x;
	text-align:center;
	/* No margin, border, or padding allowed */
		/*width:100%;*/
}
#splitter .hsplitbar.oam_active, #MySplitter .hsplitbar:hover {
	background: #ddd url(../img/splitter_bg_hover.png) repeat-x;
}

/******************
DIALOG
******************/
.ui-widget-overlay {
	position: absolute; 
	top: 0; 
	left: 0; 
	width: 100%; 
	height: 100%; 
	background: #000; 
	opacity: .50;
	filter:Alpha(Opacity=50);
}

.ui-dialog {
	position: absolute;
	width: 800px;
	overflow: hidden;
	background: #e6e6e6;
}
.ui-dialog .ui-dialog-titlebar { 
	position: relative;
	background: url(../img/dialog_header_bg.png) repeat-x top;
	color: #ffffff;
	font-weight: bold;
	height:30px;
}

.ui-dialog .ui-dialog-title {
	float: left;
	margin: 6px 10px;
	font-size:14px;
}

.ui-dialog .ui-dialog-titlebar-close {
	position: absolute;
	right: 5px;
	top: 6px;
	width: 19px;
	height: 18px;
}

.ui-dialog .ui-dialog-titlebar-close span {
	display: block;
	color: #fff;
	font-size:14px;
}

.ui-dialog .ui-dialog-titlebar-close:hover, .ui-dialog .ui-dialog-titlebar-close:focus {
	 /*padding: 0;*/
}

.ui-dialog .ui-dialog-content {
	border: 0; 
	background: #fafafa url("../img/widgets_bg.png") bottom repeat-x;
	overflow: hidden;
	zoom: 1;
	padding: 10px 10px 20px 10px;
}

.ui-dialog .ui-dialog-buttonpane {
	text-align: left;
	border-width: 1px 0 0 0;
	border-color: #ccc;
	background: #ddd;
	height:20px;
	padding: 7px 10px 8px 7px;
	position:relative;
}

.ui-dialog .ui-dialog-buttonpane button {
	float: right;
	cursor: pointer;
	width:auto;
	overflow:visible; 
}

.ui-widget input, .ui-widget select, .ui-widget textarea, .ui-widget button {
	font-family: Arial, sans-serif;
	font-size: 12px;
}

.ui-widget-content {
	border: 1px solid #5c6666;
	background: #eeeeee;
	color: #333333;
}	

