	}
	
	/* Breadcrumb */	
	div#maincontent .subheader div#breadcrumb{
		margin-left:10px;
		display:inline;
		float: left;
		display:none;
	}
	
		div#maincontent .subheader div#breadcrumb button{
			/*margin-left: 5px;*/
			display:block;
			height:28px;
			float:left;
			overflow: visible;
			color:#333333;
			background:url("../img/breadcrumb_right.png") right no-repeat;
 				white-space:nowrap;
 				padding:0 18px 0 0;
		}
		
		div#maincontent .subheader div#breadcrumb button span.bc_left{
			padding:6px 5px 6px 6px;
			line-height:28px;
		}
		
		/* Hover State*/
		
		div#maincontent .subheader div#breadcrumb button:hover{
			background:url("../img/breadcrumb_hover_right.png") right no-repeat;
		}
		
		div#maincontent .subheader div#breadcrumb button:hover span.bc_left{
			background:url("../img/breadcrumb_hover_left.png") left no-repeat;
		}
		
		/* Active State */
		div#maincontent .subheader div#breadcrumb button.selected{
			background:url("../img/breadcrumb_hover_right.png") right no-repeat;
		}
		
		div#maincontent .subheader div#breadcrumb button.selected span.bc_left{
			background:url("../img/breadcrumb_hover_left.png") left no-repeat;
		}
		
	/* Expand/Collapse States */		
	div#maincontent .subheader button.expand_collapse{
		position: absolute;
		right: 0;
		top: 0;
		height: 28px;
		width: 25px;
	}
	
	div#maincontent .subheader button#btn_expand_colbrowser{
		display:none;
	}
	
	div#maincontent .subheader button#btn_expand_results{
		display:none;
	}
	
	div#maincontent div#detail_nav button{
		padding: 0 12px 0 10px;
		height:28px;
		background:url("../img/submenu_std.png") right no-repeat;
		margin:0;
		border:0;
		float:left;
		color:#333;
	}
	
		div#maincontent div#detail_nav button:hover{
			background:url("../img/submenu_hover.png") right no-repeat;
		}
	
		div#maincontent div#detail_nav button.oam_active {
			font-weight:bold;
			border-left:4px solid #5c6666;
			background:url("../img/submenu_active.png") right no-repeat;
		}

/******************
COLUMNBROWSER
******************/
.column_bg {
	width:185px; 
	background:url("../img/col_divider.png") right repeat-y;
	height:175px;
	position:absolute;
	top:0;
	z-index:0;
	margin-top:104px;
}

/*Top level container - set the width, height and border here */
.containerobj {
	border: 1px solid #949999;
    height:175px;
    overflow-x:auto;
    overflow-y:hidden;
    white-space:nowrap;
    position:relative;
}

	/* First Column */
	.containerobj .top {
		background:url("../img/1st_column_bg.png") bottom no-repeat;
	}

	/*Div containing an individual level of the menu hierarchy*/
	.containerobj div {
	    height:100%;
	    overflow-y:auto;
	    overflow-x:hidden;
	    position:absolute;
	    background:#F9FAFA url("../img/col_divider.png") right repeat-y; /* Overwrite placeholder columns*/
	    width:180px;
	    padding-right:5px;
	}

	/*Link*/
	.containerobj a {
	 	display:block;
	  	clear:both;
	  	white-space:nowrap;
	  	padding-left:10px;
	  	font-size: 11px;
	  	color:black;
	  	height:21px;
	  	padding-top:7px;
	    padding-right:15px;
	    min-width: 155px;
	}          

		.containerobj a canvas{
		  padding-left:1em;
		}

	.containerobj .oam_active {
		color: #fff;
		background:url("../img/selection.png") bottom repeat-x;
	}

	.containerobj .inpath {
		background-color:#d8dcdc;
		color:#000;
	}

	.containerobj .hasChildMenu .widget{
		position:absolute;
	    right:0;
		margin-right:5px;
		background:url("../img/ri_arrow_black.png") no-repeat;
		margin-top: 2px;
	}
	
	.containerobj .oam_active .widget{
		background:url("../img/ri_arrow_white.png") no-repeat;
	}


