@CHARSET "UTF-8";

/*****************
CSS RESET
*****************/

* {
	vertical-align: baseline;
	font-family: Arial, sans-serif;
	font-size: 12px;
	padding: 0;
	margin: 0;
}

img {
	border: 0 none;
}

a {
	text-decoration: none;
	color: #4d4d4d;
}

ul{
	list-style-type: none;
}

button{
	border:none;
	background:none;
	/* Fix IE7 button width */
	width:auto;
   	overflow:visible;	
}

/* Avoid 3 px gap in Firefox*/
button::-moz-focus-inner { padding: 0px; margin: 0px;}
button::-moz-focus-inner { border: none;} 

button:hover, button:focus{
	cursor: pointer;
}

/********************
FLOATING
********************/

.floatLeft {
	float: left;
}

.floatRight {
	float: right;
}

.clearLeft {
	clear: left;
}

.clearRight {
	clear: right;
}

.clearBoth {
	clear: both;
}

/******************
HTML / BODY
******************/

html, body {
	background: #e6e6e6;
	height: 100%;
	overflow:hidden;
} 
/******************
HEADER
******************/

div#header {
	position: relative;
	background: #ebebeb;
	height: 74px;
	background:url("../img/header_bg.png") repeat-x;
	min-width: 1040px;
}

div#header div#logo {
	position: absolute;
	left: 0;
	height:74px;
	width:210px;
	background:url("../img/logo2.gif") no-repeat;
}
	
	div#header div#header_bg_ri {
		position: absolute;
		right: 0;
		height:74px;
		width:580px;
		background:url("../img/header_bg_ri.png") no-repeat;
	}
	
	/* Top Level Navigation */
	div#header div#toplevel_nav {
		position: absolute;
		left: 236px;
		top:10px;
	}
	
		div#header div#toplevel_nav .oam_tab{
			margin: 0 5px 0 5px;
			height: auto;
			padding:0 15px 0 0;
			width:auto; 
  			overflow:visible;
  			color: #4d4d4d;
		}	
		
			div#header div#toplevel_nav .oam_tab .button_left {
				text-align:center;
				padding:0 0 0 15px;
				height:64px;
				position:relative; 
  				display:block; 
  				white-space:nowrap;
			}
			
			/* IE7 only*/
			* + html div#header div#toplevel_nav .oam_tab .button_left {
   				margin-top:1px;
			}
				
				div#header div#toplevel_nav .oam_tab .button_left img {
					height: 24px;
					width: 24px;
					display: block;
					margin: auto;
					padding: 10px 0 5px 0;
				}			
			
			/* Hover and active State */
	
.oam_noaccess {
	opacity:0.3;
	filter:alpha(opacity=30);
}			
div#header div#toplevel_nav .oam_tab_std:hover{
	background:url("../img/tab_right_hover_bg.png") right no-repeat;
}

div#header div#toplevel_nav .oam_tab_std:hover .button_left {
	background:url("../img/tab_left_hover_bg.png") left no-repeat;
}
			
			div#header div#toplevel_nav .oam_tab_active{
				background:url("../img/tab_right_bg.png") right no-repeat;
				color:#333333;
				font-weight:bold;
			}
			
			div#header div#toplevel_nav .oam_tab_active .button_left {
				background:url("../img/tab_left_bg.png") left no-repeat;
			}
	
	/* Meta Navigation */
	div#header div#meta_nav {
		position: absolute;
		margin-right: 25px;
		right: 0;
		top:15;
		text-align: right;
		width: 200px;
		height:74px;
		background: url("../img/header_divider.png") left no-repeat;
	}
	
		div#header div#meta_nav button{
			margin: 15px 0 7px 20px;
			padding: 0 0 0 22px;
			height: 16px;
		}
		
		
			div#header div#meta_nav button#help{
				background: url("../img/icons/help_16.png") left no-repeat;
			}
			
			div#header div#meta_nav button#logout{
				background: url("../img/icons/logout_16.png") left no-repeat;
			}

		div#header div#meta_nav div#search{
			width:200px;
			height:22px;
			padding-left:20px;
		}

			div#header div#meta_nav div#search input#input_search{
				border-right:none;
				float:left;
				width:150px;
				display: inline;
			}
			
			div#header div#meta_nav div#search button#btn_search{
				width: 25px;
				height:22px;
				background:url("../img/btn_search_std.png") left no-repeat;
				margin:0;
				padding:0;
				border:0;
				float:left;
				display: inline;
			}
		
			div#header div#meta_nav div#search button#btn_search:hover{
				background:url("../img/btn_search_hover.png") left no-repeat;
			}	

