/******************
WIDGETS
******************/	

.widgets {
	margin: 25px 0 0 25px;	
	min-width: 1000px;
}

	.widgets .widgetShadow{
		background:url("../img/widget_shadow.png") bottom no-repeat;
		width: 540px;
		margin:0 0 0 -40px; /* decreases space between widgets => overlapping shadows */
	}

		.widgets .widgetShadow .widgetContainer{
			margin: 0 40px 39px 40px;
		}
		
			.widgets .widgetShadow .widgetContainer .widgetHeader {
				background:url("../img/widget_header_std.png") bottom no-repeat;
				width: 460px;
				height: 26px;
			}
			
				.widgets .widgetShadow .widgetContainer .widgetHeader h3 {
					font-size:12px;
					color:#2f3333;
					padding: 5px 0 0 18px;
				}
				
				.widgets .widgetShadow .widgetContainer .widgetHeader button {
					margin: 6px 6px 0 0;
				}
				
				.widgets .widgetShadow .widgetContainer .widgetHeader button.widget_edit{
					background:url("../img/options_std.png") no-repeat;
					height: 14px;
					width: 14px;
				}
			
			.widgets .widgetShadow .widgetContainer .widgetContent {
				border-left: 1px solid #e0e8e8;
				border-right: 1px solid #e0e8e8;
				border-bottom: 1px solid #e0e8e8;
				padding: 10px;
				background-color:#fafafa;
				min-height:65px;
			}
			
			/* Link Lists*/
			.widgets .widgetShadow .widgetContainer .widgetContent ul {
				margin-left:70px;
			}
						
				.widgets .widgetShadow .widgetContainer .widgetContent ul li {
					padding:3px 0;
				}
				
					.widgets .widgetShadow .widgetContainer .widgetContent ul li a{
						text-decoration:underline;
						color:#294d6f;
					}
				
		/********** ACTIVE STATE ************/
		.widgets .widgetShadow .oam_active .widgetHeader {
			background:url("../img/widget_header_active.png") bottom no-repeat;
			cursor: move;
		}
				
			.widgets .widgetShadow .oam_active .widgetHeader h3 {
						color:#fff;
					}
			
			.widgets .widgetShadow .oam_active .widgetHeader button.widget_edit {
						background:url("../img/options_active.png") no-repeat;
					}
					
		.widgets .widgetShadow .oam_active .widgetContent {
			border-color: #a6adad;
			cursor: move;
		}
